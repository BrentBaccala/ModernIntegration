Cell[CellGroupData[{

Cell["Notation", "Section"],

Cell[TextData[{
  "\[FilledSmallSquare]\t",
  Cell[BoxData[
      \(TraditionalForm\`lc(P(x))\)]],
  " is the coefficient of the largest power of ",
  StyleBox["x",
    FontSlant->"Italic"],
  " in the polynomial ",
  StyleBox["P",
    FontSlant->"Italic"],
  ". We can find this in ",
  StyleBox["Mathematica",
    FontSlant->"Italic"],
  " using"
}], "Text"],

Cell[BoxData[
    \(lc[f_, x_] := 
      If[\(! FreeQ[f, x]\), Coefficient[f, x\^Exponent[f, x]], f]\)], "Input",\

  InitializationCell->True],

Cell[TextData[{
  "\[FilledSmallSquare]\t",
  Cell[BoxData[
      \(TraditionalForm\`deg(P(x))\)]],
  " is the degree of the polynomail ",
  Cell[BoxData[
      \(TraditionalForm\`\(\(P(x)\)\(.\)\)\)]],
  " We can find the degree of a polynomial in ",
  StyleBox["Mathematica",
    FontSlant->"Italic"],
  " using"
}], "Text"],

Cell[BoxData[
    \(Exponent[P, x]\)], "Input"],

Cell[TextData[{
  "\[FilledSmallSquare]\t",
  Cell[BoxData[
      \(TraditionalForm\`content \((P \((x)\))\)\)]],
  " is the content of ",
  Cell[BoxData[
      \(TraditionalForm\`\(\(P(x)\)\(,\)\)\)]],
  " where ",
  Cell[BoxData[
      \(TraditionalForm\`P(
          x) = \[Sum]\+\(i = 0\)\%n\( a\_i\) x\^i\ \  \[Element] 
          D[x]\\\(\({0}\)\(.\)\)\)]],
  " We can find the content in ",
  StyleBox["Mathematica",
    FontSlant->"Italic"],
  " using"
}], "Text"],

Cell[BoxData[
    \(\(Content[P_, x_] := 
        If[GCD @@ CoefficientList[P, x] \[Equal] 0, 1, 
          GCD @@ CoefficientList[P, x]];\)\)], "Input",
  InitializationCell->True],

Cell[TextData[{
  "\[FilledSmallSquare]\t",
  Cell[BoxData[
      \(TraditionalForm\`pp(P(x))\)]],
  " is the primitive part of the polynomial ",
  StyleBox["P",
    FontSlant->"Italic"],
  ". This primitive part is found by ",
  Cell[BoxData[
      \(TraditionalForm\`pp(
          P(x)) = \(\(\(P(x)\)\/\(Content(P(x))\)\)\(.\)\)\)]],
  " So we find this in ",
  StyleBox["Mathematica",
    FontSlant->"Italic"],
  " using"
}], "Text"],

Cell[BoxData[
    \(pp[P_, x_] := P\/Content[P, x]\)], "Input",
  InitializationCell->True]
}, Open  ]],

Cell[CellGroupData[{

Cell["Algebraic Preliminaries", "Section"],

Cell[CellGroupData[{

Cell["Euclidean Division", "Subsection"],

Cell["p. 8, Bronstein, Symbolic Integration 1", "Text"],

Cell[TextData[{
  "Given a field ",
  Cell[BoxData[
      \(TraditionalForm\`K\)]],
  " and ",
  Cell[BoxData[
      \(TraditionalForm\`A, B\  \[Element] \ K\ [x]\)]],
  " woth ",
  Cell[BoxData[
      \(TraditionalForm\`\(\(B \[NotEqual] 0\)\(,\)\)\)]],
  " return ",
  Cell[BoxData[
      \(TraditionalForm\`Q, R\  \[Element] \ K\ [x]\)]],
  " such that ",
  Cell[BoxData[
      \(TraditionalForm\`A = B\ Q + R\)]],
  " and either ",
  Cell[BoxData[
      \(TraditionalForm\`R = 0\)]],
  " or ",
  Cell[BoxData[
      \(TraditionalForm\`deg(R)\  < \ \(\(deg(B)\)\(.\)\)\)]]
}], "Text"],

Cell[BoxData[
    \(PolyDivide[a_, b_, var_] := 
      Module[{A = a, B = b, x = var, Q, R, deg, T}, \[IndentingNewLine]Q = 
          0; \[IndentingNewLine]R = A; \[IndentingNewLine]deg = 
          Exponent[R, x] - Exponent[B, x]; \[IndentingNewLine]While[
          R =!= 0 && 
            deg \[GreaterEqual] 
              0, \[IndentingNewLine]T = \(lc[R, x]\/lc[B, x]\) 
              x\^deg; \[IndentingNewLine]Q = 
            Expand[Q + T]; \[IndentingNewLine]R = 
            Expand[R - B\ T]; \[IndentingNewLine]deg = 
            Exponent[R, x] - 
              Exponent[B, 
                x];\[IndentingNewLine]]; \[IndentingNewLine] \
(*\(Print["\<Quotient = \>", 
              Q];\)*) \[IndentingNewLine] (*\(Print["\<Remainder = \>", 
              R];\)*) \[IndentingNewLine]Return[{Q, 
            R}];\[IndentingNewLine]]\)], "Input",
  InitializationCell->True],

Cell[CellGroupData[{

Cell[BoxData[
    \(PolyDivide[3\ x\^3 + x\^2 + x + 5, 5\ x\^2 - 3\ x + 1, x]\)], "Input"],

Cell[BoxData[
    \({14\/25 + \(3\ x\)\/5, 111\/25 + \(52\ x\)\/25}\)], "Output"]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
    \(PolynomialQuotient[3\ x\^3 + x\^2 + x + 5, 5\ x\^2 - 3\ x + 1, 
      x]\)], "Input"],

Cell[BoxData[
    \(14\/25 + \(3\ x\)\/5\)], "Output"]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
    \(PolynomialRemainder[3\ x\^3 + x\^2 + x + 5, 5\ x\^2 - 3\ x + 1, 
      x]\)], "Input"],

Cell[BoxData[
    \(111\/25 + \(52\ x\)\/25\)], "Output"]
}, Open  ]]
}, Closed]],

Cell[CellGroupData[{

Cell["Pseudo-Division", "Subsection"],

Cell["p. 9, Bronstein, Symbolic Integration 1", "Text"],

Cell[TextData[{
  "Given an integral domain ",
  StyleBox["D",
    FontSlant->"Italic"],
  " and ",
  Cell[BoxData[
      \(TraditionalForm\`A, B\  \[Element] \ D\ [x]\)]],
  " with ",
  Cell[BoxData[
      \(TraditionalForm\`\(\(B \[NotEqual] 0\)\(,\)\)\)]],
  " return the pseudo quotient and pseudo remainder of ",
  StyleBox["A ",
    FontSlant->"Italic"],
  "and ",
  StyleBox["B",
    FontSlant->"Italic"],
  "."
}], "Text"],

Cell[BoxData[
    \(PolyPseudoDivide[a_, b_, var_] := 
      Module[{A = a, B = b, x = var, w, n, Q, R, deg, 
          T}, \[IndentingNewLine]w = lc[B, x]; \[IndentingNewLine]n = 
          Exponent[A, x] - Exponent[B, x] + 1; \[IndentingNewLine]Q = 
          0; \[IndentingNewLine]R = A; \[IndentingNewLine]deg = 
          Exponent[R, x] - Exponent[B, x]; \[IndentingNewLine]While[
          R =!= 0 && deg \[GreaterEqual] 0, \[IndentingNewLine]T = 
            lc[R, x] x\^deg; \[IndentingNewLine]n = 
            n - 1; \[IndentingNewLine]Q = 
            Expand[w\ Q + T]; \[IndentingNewLine]R = 
            Expand[w\ R - T\ B]; \[IndentingNewLine]deg = 
            Exponent[R, x] - 
              Exponent[B, 
                x];\[IndentingNewLine]]; \[IndentingNewLine] \
(*\(Print["\<Pseudo Quotient = \>", 
              w\^n\ Q];\)*) \[IndentingNewLine] (*\(Print["\<Pseudo Remainder \
= \>", w\^n\ R];\)*) \[IndentingNewLine]Return[{w\^n\ Q, 
            w\^n\ R}];\[IndentingNewLine]]\)], "Input",
  InitializationCell->True],

Cell[CellGroupData[{

Cell[BoxData[
    \(PolyPseudoDivide[3\ x\^3 + x\^2 + x + 5, 5\ x\^2 - 3\ x + 1, 
      x]\)], "Input"],

Cell[BoxData[
    \({14 + 15\ x, 111 + 52\ x}\)], "Output"]
}, Open  ]]
}, Closed]],

Cell[CellGroupData[{

Cell["Euclidean Algorithm", "Subsection"],

Cell["p. 10, Bronstein, Symbolic Integration 1", "Text"],

Cell[TextData[{
  "Given a Euclidean domain ",
  StyleBox["D",
    FontSlant->"Italic"],
  " and ",
  Cell[BoxData[
      \(TraditionalForm\`a, b\  \[Element] \ D, \)]],
  " return the greatest common divisor of ",
  StyleBox["a ",
    FontSlant->"Italic"],
  "and ",
  StyleBox["b",
    FontSlant->"Italic"],
  "."
}], "Text"],

Cell[BoxData[
    \(\(Euclidean[a_, b_, var_] := 
        Module[{A = a, B = b, x = var, q, r}, \[IndentingNewLine]While[
            B =!= 0, \[IndentingNewLine]q = \(PolyDivide[A, B, 
                  x]\)[\([1]\)]; \[IndentingNewLine]r = \(PolyDivide[A, B, 
                  x]\)[\([2]\)]; \[IndentingNewLine]A = 
              B; \[IndentingNewLine]B = 
              r;\[IndentingNewLine]]; \[IndentingNewLine]Return[
            A];\[IndentingNewLine]];\)\)], "Input",
  InitializationCell->True],

Cell[CellGroupData[{

Cell[BoxData[
    \(Euclidean[x\^4 - 2\ x\^3 - 6\ x\^2 + 12\ x + 15, 
      x\^3 + x\^2 - 4\ x - 4, x]\)], "Input"],

Cell[BoxData[
    \(5 + 5\ x\)], "Output"]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
    \(PolynomialGCD[x\^4 - 2\ x\^3 - 6\ x\^2 + 12\ x + 15, 
      x\^3 + x\^2 - 4\ x - 4]\)], "Input"],

Cell[BoxData[
    \(1 + x\)], "Output"]
}, Open  ]]
}, Closed]],

Cell[CellGroupData[{

Cell["Greatest Common Divisor", "Subsection"],

Cell[TextData[{
  "Tests on the Euclidean algorithm show that is does not agree with ",
  StyleBox["Mathematica",
    FontSlant->"Italic"],
  "'s results. Noting that dividing the polynomial remainder by its primitive \
every time the algorithm cycles results in the correct outcome."
}], "Text"],

Cell["\<\
For an efficient GCD computation see p. 20, Symbolic Integration in Java. \
\>", "Text"],

Cell[BoxData[
    \(\(GreatestCommonDivisor[a_, b_, var_] := 
        Module[{A = a, B = b, x = var, q, r, rtemp}, \[IndentingNewLine]While[
            B =!= 0, \[IndentingNewLine]q = \(PolyDivide[A, B, 
                  x]\)[\([1]\)]; \[IndentingNewLine]r = 
              Simplify[
                pp[\(PolyDivide[A, B, x]\)[\([2]\)], 
                  x]]; \[IndentingNewLine] (*\(Print["\<r = \>", 
                  r];\)*) \[IndentingNewLine]A = B; \[IndentingNewLine]B = 
              r;\[IndentingNewLine]]; \[IndentingNewLine]Return[
            A];\[IndentingNewLine]];\)\)], "Input",
  InitializationCell->True],

Cell[CellGroupData[{

Cell[BoxData[
    \(GreatestCommonDivisor[x\^4 - 2\ x\^3 - 6\ x\^2 + 12\ x + 15, 
      x\^3 + x\^2 - 4\ x - 4, x]\)], "Input"],

Cell[BoxData[
    \(1 + x\)], "Output"]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
    \(GreatestCommonDivisor[x\^12 + 7\ x + 1, 5\ x\^11 + 11  x + 1, 
      x]\)], "Input"],

Cell[BoxData[
    \(1\)], "Output"]
}, Open  ]]
}, Closed]],

Cell[CellGroupData[{

Cell["Extended Euclidean", "Subsection"],

Cell["p. 11, Bronstein, Symbolic Integration 1", "Text"],

Cell[TextData[{
  "Given a Euclidean domain ",
  StyleBox["D",
    FontSlant->"Italic"],
  " and ",
  Cell[BoxData[
      \(TraditionalForm\`a, b\  \[Element] \ D, \)]],
  " return ",
  Cell[BoxData[
      \(TraditionalForm\`s, t\  \[Element] \ D\)]],
  " such that ",
  Cell[BoxData[
      \(TraditionalForm\`g = gcd(a, b)\)]],
  " and ",
  Cell[BoxData[
      \(TraditionalForm\`s\ a + t\ b = \(\(g\)\(.\)\)\)]]
}], "Text"],

Cell[BoxData[
    \(\(ExtendedEuclidean[A_, B_, var_] := 
        Module[{a = A, b = B, x = var, a1, a2, b1, b2, q, r, r1, 
            r2}, \[IndentingNewLine]a1 = 1; a2 = 0; b1 = 0; 
          b2 = 1; \[IndentingNewLine]While[
            b =!= 0, \[IndentingNewLine]q = \(PolyDivide[a, b, 
                  x]\)[\([1]\)]; \[IndentingNewLine]r = \(PolyDivide[a, b, 
                  x]\)[\([2]\)]; \[IndentingNewLine]a = 
              Expand[b]; \[IndentingNewLine]b = r; \[IndentingNewLine]r1 = 
              a1 - q\ b1; \[IndentingNewLine]r2 = 
              a2 - q\ b2; \[IndentingNewLine]a1 = 
              Expand[b1]; \[IndentingNewLine]a2 = 
              Expand[b2]; \[IndentingNewLine]b1 = r1; \[IndentingNewLine]b2 = 
              r2;\[IndentingNewLine]]; \[IndentingNewLine]Return[{a1, a2, 
              a}];\[IndentingNewLine]];\)\)], "Input",
  InitializationCell->True],

Cell[CellGroupData[{

Cell[BoxData[
    \(ExtendedEuclidean[x\^4 - 2\ x\^3 - 6\ x\^2 + 12\ x + 15, 
      x\^3 + x\^2 - 4\ x - 4, x]\)], "Input"],

Cell[BoxData[
    \({3 - x, 10 - 6\ x + x\^2, 5 + 5\ x}\)], "Output"]
}, Open  ]]
}, Closed]],

Cell[CellGroupData[{

Cell["Half Extended Euclidean Algorithm", "Subsection"],

Cell["p. 11, Bronstein, Symbolic Integration 1", "Text"],

Cell[TextData[{
  "Given a Euclidean domain ",
  StyleBox["D",
    FontSlant->"Italic"],
  " and ",
  Cell[BoxData[
      \(TraditionalForm\`a, b\  \[Element] \ D, \)]],
  " return ",
  Cell[BoxData[
      \(TraditionalForm\`s, g\  \[Element] \ D\)]],
  " such that ",
  Cell[BoxData[
      \(TraditionalForm\`g = gcd(a, b)\)]],
  " and ",
  Cell[BoxData[
      \(TraditionalForm\`s\ a \[Congruent] g\ \(\((mod\ b)\)\(.\)\)\)]]
}], "Text"],

Cell[BoxData[
    \(\(HalfExtendedEuclidean[A_, B_, var_] := 
        Module[{a = A, b = B, x = var, a1, b1, q, r}, \[IndentingNewLine]a1 = 
            1; b1 = 0; \[IndentingNewLine]While[
            b =!= 0, \[IndentingNewLine]q = \(PolyDivide[a, b, 
                  x]\)[\([1]\)]; \[IndentingNewLine]r = \(PolyDivide[a, b, 
                  x]\)[\([2]\)]; \[IndentingNewLine]a = 
              Expand[b]; \[IndentingNewLine]b = r; \[IndentingNewLine]r1 = 
              a1 - q\ b1; \[IndentingNewLine]a1 = 
              Expand[b1]; \[IndentingNewLine]b1 = 
              r1;\[IndentingNewLine]]; \[IndentingNewLine]Return[{a1, 
              a}];\[IndentingNewLine]];\)\)], "Input",
  InitializationCell->True],

Cell["This code is tested as a part of the next program.", "Text"]
}, Closed]],

Cell[CellGroupData[{

Cell["Extended Euclidean Algorithm \"Half / Full\" Version", "Subsection"],

Cell["p. 12, Bronstein, Symbolic Integration 1", "Text"],

Cell[TextData[{
  "Given a Euclidean domain ",
  StyleBox["D",
    FontSlant->"Italic"],
  " and ",
  Cell[BoxData[
      \(TraditionalForm\`a, b\  \[Element] \ D\)]],
  Cell[BoxData[
      \(TraditionalForm\`, \)]],
  " return ",
  Cell[BoxData[
      \(TraditionalForm\`s, t, g\  \[Element] \ D\)]],
  " such that ",
  Cell[BoxData[
      \(TraditionalForm\`\(\(g = gcd(a, b)\)\(,\)\)\)]],
  " and ",
  Cell[BoxData[
      \(TraditionalForm\`s\ a + t\ b = \(\(g\)\(.\)\)\)]]
}], "Text"],

Cell[BoxData[
    \(\(ExtendedEuclideanHalfFull[A_, B_, var_] := 
        Module[{a = A, b = B, x = var, s, g, t, r}, \[IndentingNewLine]s = 
            Expand[\(HalfExtendedEuclidean[a, b, 
                  x]\)[\([1]\)]]; \[IndentingNewLine]g = 
            Expand[\(HalfExtendedEuclidean[a, b, 
                  x]\)[\([2]\)]]; \[IndentingNewLine]t = 
            Expand[\(PolyDivide[g - s\ a, b, 
                  x]\)[\([1]\)]]; \[IndentingNewLine]r = \(PolyDivide[
                g - s\ a, b, x]\)[\([2]\)]; \[IndentingNewLine]If[
            r \[NotEqual] 0, Print["\<Error @ ExtendedEuclideanalfFull\>"]; 
            Return[];]; \[IndentingNewLine]Return[{s, t, 
              g}];\[IndentingNewLine]];\)\)], "Input",
  InitializationCell->True],

Cell[CellGroupData[{

Cell[BoxData[
    \(ExtendedEuclideanHalfFull[x\^4 - 2\ x\^3 - 6\ x\^2 + 12\ x + 15, 
      x\^3 + x\^2 - 4\ x - 4, x]\)], "Input"],

Cell[BoxData[
    \({3 - x, 10 - 6\ x + x\^2, 5 + 5\ x}\)], "Output"]
}, Open  ]],

Cell["This agrees with Bronstein p. 13.", "Text"]
}, Closed]],

Cell[CellGroupData[{

Cell["Extended Euclidean Algorithm Diophantine Version", "Subsection"],

Cell["p. 13, Bronstein, Symbolic Integration 1", "Text"],

Cell[TextData[{
  "Given a Euclidean domain ",
  StyleBox["D",
    FontSlant->"Italic"],
  " and ",
  Cell[BoxData[
      \(TraditionalForm\`a, b, c\  \[Element] \ D\)]],
  " with ",
  Cell[BoxData[
      \(TraditionalForm\`\(\(c\  \[Element] \ \((a, b)\)\)\(,\)\)\)]],
  " return ",
  Cell[BoxData[
      \(TraditionalForm\`s, t\  \[Element] \ D\)]],
  " such that ",
  Cell[BoxData[
      \(TraditionalForm\`s\ a + t\ b = c\)]],
  " and either ",
  Cell[BoxData[
      \(TraditionalForm\`s = 0\)]],
  " or ",
  Cell[BoxData[
      \(TraditionalForm\`v(s) < \(\(v(b)\)\(.\)\)\)]]
}], "Text"],

Cell[BoxData[
    \(\(ExtendedEuclideanDiophantine[A_, B_, C_, var_] := 
        Module[{a = A, b = B, c = C, x = var, s, t, g, q, 
            r}, \[IndentingNewLine]s = 
            Expand[\(ExtendedEuclideanHalfFull[a, b, 
                  x]\)[\([1]\)]]; \[IndentingNewLine]t = 
            Expand[\(ExtendedEuclideanHalfFull[a, b, 
                  x]\)[\([2]\)]]; \[IndentingNewLine]g = \
\(ExtendedEuclideanHalfFull[a, b, 
                x]\)[\([3]\)]; \[IndentingNewLine]q = \(PolyDivide[c, g, 
                x]\)[\([1]\)]; \[IndentingNewLine]r = \(PolyDivide[c, g, 
                x]\)[\([2]\)]; \[IndentingNewLine]If[
            r =!= 0, \[IndentingNewLine]Print["\<Error @ \
ExtendedEuclideanDiophantine\>"]; \[IndentingNewLine]Return[];\
\[IndentingNewLine]]; \[IndentingNewLine]s = q\ s; \[IndentingNewLine]t = 
            q\ t; \[IndentingNewLine]If[
            s =!= 0 && 
              Exponent[s, x] \[GreaterEqual] 
                Exponent[b, x], \[IndentingNewLine]q = \(PolyDivide[s, b, 
                  x]\)[\([1]\)]; \[IndentingNewLine]r = \(PolyDivide[s, b, 
                  x]\)[\([2]\)]; \[IndentingNewLine]s = 
              r; \[IndentingNewLine]t = 
              Expand[t + 
                  q\ a];\[IndentingNewLine]]; \[IndentingNewLine]Return[{s, 
              t}]\[IndentingNewLine]];\)\)], "Input",
  InitializationCell->True],

Cell[CellGroupData[{

Cell[BoxData[
    \(ExtendedEuclideanDiophantine[x\^2 - 2\ x + 1, x + 1, x\^2 + 3\ x, 
      x]\)], "Input"],

Cell[BoxData[
    \({\(-\(1\/2\)\), 1\/2 + \(3\ x\)\/2}\)], "Output"]
}, Open  ]],

Cell["This agrees with Bronstein p. 15.", "Text"],

Cell[CellGroupData[{

Cell[BoxData[
    \(ExtendedEuclideanDiophantine[\(-5\)\ x\^2 - 2, x\^3 + 2\ x, 
      x\^7 - 24\ x\^4 - 4\ x\^2 + 8\ x - 8, x]\)], "Input"],

Cell[BoxData[
    \({4 + 8\ x\^2, 4 + 16\ x - 2\ x\^2 + x\^4}\)], "Output"]
}, Open  ]],

Cell["This agrees with Bronstein p. 44.", "Text"]
}, Closed]],

Cell[CellGroupData[{

Cell["Half Extended Euclidean Diophantine Version", "Subsection"],

Cell["p. 13, Bronstein, Symbolic Integration 1", "Text"],

Cell[TextData[{
  "Given a Euclidean domain ",
  StyleBox["D",
    FontSlant->"Italic"],
  " and ",
