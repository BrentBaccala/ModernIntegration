\[IndentingNewLine]]; \[IndentingNewLine]s = q\ s; \[IndentingNewLine]If[
            s =!= 0 && 
              Exponent[s, x] \[GreaterEqual] 
                Exponent[b, x], \[IndentingNewLine]q = \(PolyDivide[s, b, 
                  x]\)[\([1]\)]; \[IndentingNewLine]r = \(PolyDivide[s, b, 
                  x]\)[\([2]\)]; \[IndentingNewLine]s = 
              r;\[IndentingNewLine]]; \[IndentingNewLine]Return[
            s];\[IndentingNewLine]];\)\)], "Input",
  InitializationCell->True]
}, Closed]],

Cell[CellGroupData[{

Cell["Partial Fraction Decomposition", "Subsection"],

Cell["p. 15, Bronstein, Symbolic Integration 1", "Text"],

Cell[TextData[{
  "Given a Euclidean Domain ",
  StyleBox["D",
    FontSlant->"Italic"],
  ", a positive integer ",
  StyleBox["n",
    FontSlant->"Italic"],
  " and ",
  Cell[BoxData[
      \(TraditionalForm\`a, d\_1, \[CenterEllipsis], 
      d\_n\  \[Element] \ D\\{0}\)]],
  " with ",
  Cell[BoxData[
      \(TraditionalForm\`gcd(d\_i, d\_j) = 1\)]],
  " for ",
  Cell[BoxData[
      \(TraditionalForm\`i \[NotEqual] j\)]],
  ", return ",
  Cell[BoxData[
      \(TraditionalForm\`a\_0, a\_1, \[CenterEllipsis], 
      a\_n\  \[Element] \ D\)]],
  " such that ",
  Cell[BoxData[
      FormBox[
        RowBox[{
          FractionBox["a", 
            
            RowBox[{\(d\_1\), \(d\_2\), \(\[CenterEllipsis]d\_n\), Cell[
              ""]}]], "=", \(a\_0 + \[Sum]\+\(i = 1\)\%n a\_i\/d\_i\)}], 
        TraditionalForm]]],
  " and either ",
  Cell[BoxData[
      \(TraditionalForm\`a\_i = 0\)]],
  " or ",
  Cell[BoxData[
      \(TraditionalForm\`v(a\_i) < v(d\_i)\)]],
  " for ",
  Cell[BoxData[
      \(TraditionalForm\`i \[GreaterEqual] 1\)]],
  "."
}], "Text"],

Cell[BoxData[
    \(\(PartialFraction[A_, B_, var_] := 
        Module[{a = A, d = B, x = var, dprod, a0, r, n, temp, a1, t, bo, 
            aall}, \[IndentingNewLine]dprod = 
            Times @@ d; \[IndentingNewLine]a0 = \(PolyDivide[a, dprod, 
                x]\)[\([1]\)]; \[IndentingNewLine]r = \(PolyDivide[a, dprod, 
                x]\)[\([2]\)]; \[IndentingNewLine]n = 
            Length[d]; \[IndentingNewLine]If[
            n == 1, \[IndentingNewLine]\(Return[{a0, 
                  r}];\), \[IndentingNewLine]temp = 
              ExtendedEuclideanDiophantine[Times @@ \((Delete[d, 1])\), 
                d[\([1]\)], r, x]; \[IndentingNewLine]a1 = 
              temp[\([1]\)]; \[IndentingNewLine]t = 
              temp[\([2]\)]; \[IndentingNewLine]b0 = \(PartialFraction[t, 
                  Delete[d, 1], 
                  x]\)[\([1]\)]; \[IndentingNewLine]aall = \(PartialFraction[
                  t, Delete[d, 1], 
                  x]\)[\([2]\)];\[IndentingNewLine]]; \
\[IndentingNewLine]Return[{a0 + b0, a1, 
              aall}];\[IndentingNewLine]];\)\)], "Input",
  InitializationCell->True],

