Cell[CellGroupData[{

Cell["SubResultant", "Subsection"],

Cell["p. 24, Bronstein, Symbolic Integration 1", "Text"],

Cell[TextData[{
  "Given an integral domain ",
  StyleBox["D",
    FontSlant->"Italic"],
  " and ",
  Cell[BoxData[
      \(TraditionalForm\`A, B\  \[Element] \ D[x]\)]],
  " with ",
  Cell[BoxData[
      \(TraditionalForm\`B \[NotEqual] 0\)]],
  " and ",
  Cell[BoxData[
      \(TraditionalForm\`\(\(deg(A) \[GreaterEqual] deg(B)\)\(,\)\)\)]],
  " return ",
  Cell[BoxData[
      \(TraditionalForm\`resultant(A, B)\)]],
  " and the subresultant PRS ",
  Cell[BoxData[
      \(TraditionalForm\`\((R\_0, R\_0, \[CenterEllipsis], R\_k, 0)\)\)]],
  " of ",
  StyleBox["A",
    FontSlant->"Italic"],
  " and ",
  StyleBox["B",
    FontSlant->"Italic"],
  "."
}], "Text"],

Cell[BoxData[
    \(\(SubResultant[a_, b_, var_] := 
        Module[{A = a, B = b, x = var, R, i, \[Gamma], \[Delta], \[Beta], 
            rnew, r, Q, c, s, ply}, \[IndentingNewLine]R = 
            Append[{A}, B]; \[IndentingNewLine]i = 
            2; \[IndentingNewLine]\[Gamma] = {\(-1\)}; \[IndentingNewLine]\
\[Delta] = {Exponent[A, x] - 
                Exponent[B, 
                  x]}; \[IndentingNewLine]\[Beta] = {\((\(-1\))\)\^\(\[Delta] \
+ 1\)}; \[IndentingNewLine]r = {lc[R[\([i]\)], x]}; \[IndentingNewLine]While[
            R[\([i]\)] =!= 0, \[IndentingNewLine]r = 
              Flatten[Append[{r}, 
                  lc[R[\([i]\)], 
                    x]]]; \[IndentingNewLine]Rem = \(PolyPseudoDivide[
                  R[\([i - 1]\)], R[\([i]\)], 
                  x]\)[\([2]\)]; \[IndentingNewLine]R = 
              Flatten[Append[{R}, 
                  Expand[Simplify[
                      Rem/\[Beta][\([i - 1]\)]]]]]; \[IndentingNewLine]If[
              R[\([i + 1]\)] \[Equal] 
                0, \(Goto[10];\)]; \[IndentingNewLine]i = 
              i + 1; \[IndentingNewLine]\[Gamma] = 
              Flatten@Append[{\[Gamma]}, \((\(-r[\([i - 1]\)]\))\)\^\[Delta][\
\([i - 2]\)]\ \[Gamma][\([i - 2]\)]\^\(1 - \[Delta][\([i - 2]\)]\)]; \
\[IndentingNewLine]\[Delta] = 
              Flatten@Append[{\[Delta]}, 
                  Exponent[R[\([i - 1]\)], x] - 
                    Exponent[R[\([i]\)], x]]; \[IndentingNewLine]\[Beta] = 
              Flatten@Append[{\[Beta]}, \(-r[\([i - 
                            1]\)]\)\ \[Gamma][\([i - 1]\)]\^\[Delta][\([i - \
1]\)]];\[IndentingNewLine]]; \[IndentingNewLine]Label[
            10]; \[IndentingNewLine]k = i - 1; \[IndentingNewLine]If[
            Exponent[R[\([k + 1]\)], x] > 0, 
            Return[Flatten[{0, R}]]]; \[IndentingNewLine]If[
            Exponent[R[\([k]\)], x] == 1, 
            Return[Flatten[{R[\([k]\)], R}]]]; \[IndentingNewLine]s = 
            1; \[IndentingNewLine]c = 
            1; \[IndentingNewLine]Do[\[IndentingNewLine]If[
              Mod[Exponent[R[\([j]\)], x], 2] \[Equal] 1 && 
                Mod[Exponent[R[\([j + 1]\)], x], 2] \[Equal] 
                  1, \[IndentingNewLine]\(s = \(-s\);\)\[IndentingNewLine]]; \
\[IndentingNewLine]c = 
              c \((\[Beta][\([j]\)]/r[\([j]\)]\^\(1 + \
\[Delta][\([j]\)]\))\)\^Exponent[R[\([j + 1]\)], x]\ \
r[\([j]\)]\^\(Exponent[R[\([j]\)], x] - Exponent[R[\([j + 2]\)], x]\);\
\[IndentingNewLine], {j, 1, k - 1}]; \[IndentingNewLine]Return[
            Flatten[{s\ c\ R[\([k + 1]\)]\^Exponent[R[\([k - 1]\)], x], 
                R}]]\[IndentingNewLine]];\)\)], "Input",
  InitializationCell->True],

Cell[CellGroupData[{

Cell[BoxData[
    \(SubResultant[x\^2 + 1, x\^2 - 1, x]\)], "Input"],

Cell[BoxData[
    \({4, 1 + x\^2, \(-1\) + x\^2, \(-2\), 0}\)], "Output"]
}, Open  ]],

