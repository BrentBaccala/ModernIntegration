Cell[CellGroupData[{

Cell["Squarefree Factorisation", "Subsection"],

Cell["p. 29, Bronstein, Symbolic Integration 1", "Text"],

Cell[TextData[{
  "Given a uniqure factorization domain ",
  StyleBox["D",
    FontSlant->"Italic"],
  " of characteristic 0 and ",
  Cell[BoxData[
      \(TraditionalForm\`\(\(A\  \[Element] \ D[x]\)\(,\)\)\)]],
  " return ",
  Cell[BoxData[
      \(TraditionalForm\`A\_1, A\_2, \[CenterEllipsis], 
      A\_m\  \[Element] \ D[x]\)]],
  " such that ",
  Cell[BoxData[
      \(TraditionalForm\`A = \[Product]\+\(k = 1\)\%m A\_k\^k\)]],
  " is a squarefree factorization of ",
  StyleBox["A",
    FontSlant->"Italic"],
  ". ",
  Cell[BoxData[
      FormBox[Cell[""], TraditionalForm]]]
}], "Text"],

Cell[BoxData[
    \(Squarefree[a_, var_] := 
      Module[{A = a, x = var, c, S, Sstar, Sneg, k, 
          Y}, \[IndentingNewLine]c = 
          PolynomialGCD @@ CoefficientList[A, x]; \[IndentingNewLine]S = 
          A\/c; \[IndentingNewLine]Sneg = 
          PolynomialGCD[S, Dt[S, x]]; \[IndentingNewLine]Sstar = 
          Simplify[S\/Sneg]; \[IndentingNewLine]If[
          Exponent[Sneg, x] \[Equal] 
            0, \(Return[A];\)]; \[IndentingNewLine]k = 
          1; \[IndentingNewLine]A = {}; \[IndentingNewLine]While[
          Exponent[Sneg, x] > 0, \[IndentingNewLine]Y = 
            Simplify[PolynomialGCD[Sstar, Sneg]]; \[IndentingNewLine]A = 
            Flatten[Append[A, 
                Simplify[\((Sstar\/Y)\)\^k]]]; \[IndentingNewLine]Sstar = 
            Y; \[IndentingNewLine]Sneg = 
            Simplify[
              Sneg\/Y]; \[IndentingNewLine]\(k++\);\[IndentingNewLine]]; \
\[IndentingNewLine]A[\([1]\)] = c\ Sneg\ A[\([1]\)]; \[IndentingNewLine]A = 
          If[k > 1, Flatten[Append[{A}, Sstar\^k]], 
            A]; \[IndentingNewLine]Return[A];\[IndentingNewLine]]\)], "Input",\

  InitializationCell->True],

Cell[CellGroupData[{

Cell[BoxData[
    \(Squarefree[x\^8 + 6\ x\^6 + 12\ x\^4 + 8\ x\^2, x]\)], "Input"],

Cell[BoxData[
